@model List<Hashtag>
<div class="col-lg-12">
    <div class="sidebar-item tags">
        <div class="sidebar-heading">
            <h2>ETİKET BULUTU</h2>
        </div>
        <div class="content">
            <ul>
                @foreach (var item in Model)
                {
                    <li><a asp-controller="Blog" asp-action="BlogByHashtagList" asp-route-hashtagName="@item.Name">@item.Name</a></li>
                }

            </ul>
        </div>
    </div>
</div>
